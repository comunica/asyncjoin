{
  "name": "asyncjoin",
  "version": "1.2.3",
  "description": "Join library for asynciterator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/asyncjoin"
  },
  "dependencies": {
    "asynciterator": "^3.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "lodash": "^4.17.21",
    "manual-git-changelog": "^1.0.0",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "test": "mocha test",
    "version": "manual-git-changelog onversion",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -d --module NodeNext --moduleResolution nodenext --outDir esm"
  },
  "pre-commit": [
    "test"
  ],
  "files": [
    "esm/**/*.js",
    "dist/**/*.js"
  ],
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    }
  }
}
